{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "lf",
	"plugins": [
		"@shopify/prettier-plugin-liquid"
	],
	"overrides": [
		{
			"files": "*.liquid",
			"options": {
				"singleQuote": false
			}
		}
	]
}
